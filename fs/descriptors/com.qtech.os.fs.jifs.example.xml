<!DOCTYPE plugin SYSTEM "jnode.dtd">

<plugin id="com.qtech.os.fs.jifs.example"
        name="Jnode Information FileSystem Extension Example"
        version="@VERSION@"
        license-name="lgpl"
        provider-name="QTechOS">
        
        
  <requires>
  	<import plugin="com.qtech.os.fs.jifs"/>
	<import plugin="com.qtech.os.fs.jifs.def"/>
  </requires>
        
  <extension point="com.qtech.os.fs.jifs.def.info">
    <info class="com.qtech.os.fs.jifs.JIFSDirectory" name="testdir"/>
    <info class="com.qtech.os.fs.jifs.JIFSFile" name="testfile"/>
  </extension>
          
</plugin>
